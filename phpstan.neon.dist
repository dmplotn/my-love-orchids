includes:
    - vendor/larastan/larastan/extension.neon

parameters:
    paths:
        - app/

    # Level 9 is the highest level
    level: 5

    # Увеличиваем лимит памяти для анализа
    tmpDir: .phpstan.cache

    ignoreErrors:
        # Add patterns to ignore here if needed

    excludePaths:
        - ./*/*/FileToBeExcluded.php
